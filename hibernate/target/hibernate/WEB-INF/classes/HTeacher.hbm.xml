<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.seecee.sc1911.pojo.HTeacher" table="H_TEACHER" schema="ADMIN">
        <id name="id" column="ID">
            <generator class="native">
                <param name="sequence_name">id_seq</param>
            </generator>
        </id>
        <property name="name" column="NAME"/>

        <bag name="ListStudent" table="STU_TEA" cascade="save-update">
            <key column="TID" ></key>
            <many-to-many column="SID" class="com.seecee.sc1911.pojo.HStudent"></many-to-many>
        </bag>
    </class>
</hibernate-mapping>