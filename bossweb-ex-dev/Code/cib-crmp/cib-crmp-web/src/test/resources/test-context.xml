<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
      xmlns:mvc="http://www.springframework.org/schema/mvc"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="
      http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
      http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd
      ">

	<mvc:annotation-driven />
	
	<bean id="RecalculationController" class="com.newland.boss.cib.crmp.web.controller.RecalculationController" />
	<bean id="indexController" class="com.newland.boss.cib.crmp.web.controller.IndexController" />
	<!-- 被依赖的服务 -->
    <bean id="queryRecalculationService" class="com.newland.boss.cib.crmp.web.service.impl.QueryRecalculationServiceImpl"></bean>
    <bean id="billSummingOrgDayService" class="com.newland.boss.cib.crmp.settle.service.impl.BillSummingOrgDayServiceImpl"></bean>
    <bean id="billSummingUsrDayService" class="com.newland.boss.cib.crmp.settle.service.impl.BillSummingUsrDayServiceImpl"></bean>
    <bean id="organizationService" class="com.newland.boss.kpi.admin.service.impl.OrganizationServiceImpl"></bean>
    <bean id="dictDefService" class="com.newland.boss.kpi.server.impl.DictDefServiceImpl"></bean>
    <bean id="appSession" class="com.newland.boss.kpi.server.impl.AppSessionImpl"></bean>
    <bean id="orgService" class="com.newland.boss.kpi.server.impl.OrgServiceImpl"></bean>
	
	<!-- 被依赖的服务 -->
    <bean id="recalcCdrTaskDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.cib.crmp.web.dao.RecalcCdrTaskDao" />
    </bean>
	<bean id="session" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="javax.servlet.http.HttpSession" />
    </bean>
	 <bean id="billSummingOrgDayDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.cib.crmp.settle.dao.BillSummingOrgDayDao" />
    </bean>
    <bean id="billSummingUsrDayDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.cib.crmp.settle.dao.BillSummingUsrDayDao" />
    </bean>
    <bean id="organizationDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.kpi.admin.dao.OrganizationDao" />
    </bean>
    <bean id="dictDefDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.kpi.dao.DictDefDao" />
    </bean>
    <bean id="orgDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.kpi.dao.OrgDao" />
    </bean>
    <bean id="operOrgRelaDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.kpi.admin.dao.OperOrgRelaDao" />
    </bean>
</beans>