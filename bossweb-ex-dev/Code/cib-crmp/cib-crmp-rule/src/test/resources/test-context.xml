<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
      xmlns:mvc="http://www.springframework.org/schema/mvc"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="
      http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
      http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd
      ">

	<mvc:annotation-driven />
	
	<bean id="rateRuleController" class="com.newland.boss.cib.crmp.controller.RateRuleController" />
	<bean id="rateRuleRelaController" class="com.newland.boss.cib.crmp.controller.RateRuleRelaController" />
	<bean id="settleApportionRuleController" class="com.newland.boss.cib.crmp.controller.SettleApportionRuleController" />
	<bean id="settleApportionRelaController" class="com.newland.boss.cib.crmp.controller.SettleApportionRelaController" />

	<!-- 被依赖的服务 -->
    <bean id="rateRuleService" class="com.newland.boss.cib.crmp.service.impl.RateRuleServiceImpl"></bean>
    <bean id="rateRuleRelaService" class="com.newland.boss.cib.crmp.service.impl.RateRuleRelaServiceImpl"></bean>
    <bean id="settleApportionRuleService" class="com.newland.boss.cib.crmp.service.impl.SettleApportionRuleServiceImpl"></bean>
	<bean id="settleApportionRelaService" class="com.newland.boss.cib.crmp.service.impl.SettleApportionRelaServiceImpl"></bean>
    <bean id="dictDefService" class="com.newland.boss.kpi.server.impl.DictDefServiceImpl"></bean>
    <bean id="operateLogServiceImpl" class="com.newland.boss.kpi.server.impl.OperateLogServiceImpl"></bean>
    
<!--    <bean id="session" class="javax.servlet.http.HttpSession"></bean> -->
	
	<!-- 被依赖的服务 -->
    <bean id="rateRuleDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.cib.crmp.dao.RateRuleDao" />
    </bean>
    <bean id="rateRuleRelaDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.cib.crmp.dao.RateRuleRelaDao" />
    </bean>
    <bean id="settleApportionRuleDao" class="org.mockito.Mockito"
		factory-method="mock">
		<constructor-arg
			value="com.newland.boss.cib.crmp.dao.SettleApportionRuleDao" />
	</bean>
	<bean id="settleApportionRelaDao" class="org.mockito.Mockito"
		factory-method="mock">
		<constructor-arg
			value="com.newland.boss.cib.crmp.dao.SettleApportionRelaDao" />
	</bean>
    <bean id="dictDefDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.kpi.dao.DictDefDao" />
    </bean>
    <bean id="operateLogDao" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.kpi.dao.OperateLogDao" />
    </bean>
   
<!--    <bean id="appSession" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="com.newland.boss.kpi.server.impl.AppSessionImpl" />
    </bean> -->
	<bean id="session" class="org.mockito.Mockito" factory-method="mock">
      <constructor-arg value="javax.servlet.http.HttpSession" />
    </bean>
	
</beans>