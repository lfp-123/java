<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:mvc="http://www.springframework.org/schema/mvc"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
      http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
      http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd
      ">

    <mvc:annotation-driven/>

    <bean id="dictDefController" class="com.newland.boss.kpi.controller.DictDefController" />

    <!-- 被依赖的服务 -->
    <bean id="dictDefService" class="com.newland.boss.kpi.server.impl.DictDefServiceImpl"/>
    <!-- 被依赖的服务 -->
    <bean id="dictDefDao" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.newland.boss.kpi.dao.DictDefDao"/>
    </bean>


</beans>